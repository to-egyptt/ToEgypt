<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.google.ToEgypt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.google.ToEgypt.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="io.google.ToEgypt.Admin" />
        <activity android:name="io.google.ToEgypt.Registration" />
        <activity android:name="io.google.ToEgypt.UpdateUser" />
        <activity android:name="io.google.ToEgypt.UpdatePlace" />
        <activity android:name="io.google.ToEgypt.UpdatePackage" />
        <activity android:name="io.google.ToEgypt.User" />
        <activity android:name="io.google.ToEgypt.Guide" />
    </application>

</manifest>